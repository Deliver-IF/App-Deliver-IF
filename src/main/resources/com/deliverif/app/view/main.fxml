<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.ChoiceBox?>

<BorderPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.deliverif.app.controller.MainController">
   <top>
      <VBox>
         <children>
            <MenuBar prefHeight="29.0" prefWidth="379.0" BorderPane.alignment="CENTER" fx:id="menuBar" >
               <menus>
                  <Menu mnemonicParsing="false" text="File">
                     <items>
                        <MenuItem fx:id="loadMapMenuItem" mnemonicParsing="false" text="Load a map" onAction="#loadMapAction"/>
                        <MenuItem fx:id="loadTourMenuItem" mnemonicParsing="false" text="Load a tour" onAction="#loadTourAction"/>
                        <MenuItem fx:id="saveTourMenuItem" mnemonicParsing="false" text="Save a tour" onAction="#saveTourAction"/>
                     </items>
                  </Menu>
                  <Menu fx:id="aboutMenu" mnemonicParsing="false" text="About">
                  </Menu>
               </menus>
            </MenuBar>
            <BorderPane style="-fx-border-style: none none solid none; -fx-border-width: 1; -fx-border-color: transparent transparent black transparent;">
               <left>
                  <Button fx:id="addDeliveryButton" mnemonicParsing="false" text="Add delivery" BorderPane.alignment="CENTER">
                     <BorderPane.margin>
                        <Insets left="10.0" />
                     </BorderPane.margin>
                  </Button>
               </left>
               <right>
                  <HBox alignment="CENTER" BorderPane.alignment="CENTER">
                     <children>
                        <Label text="Nb of coursiers:" HBox.hgrow="ALWAYS">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Label>
                        <Button fx:id="increaseCourierButton" mnemonicParsing="false" text="+" onAction="#addCourier">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Text fx:id="nbCourierText" strokeType="OUTSIDE" strokeWidth="0.0" text="0">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Text>
                        <Button fx:id="decreaseCourierButton" mnemonicParsing="false" text=" - " onAction="#deleteCourier">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                        <ComboBox fx:id="selectCourierButton" prefWidth="150.0">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </ComboBox>
                     </children>
                     <BorderPane.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </BorderPane.margin>
                  </HBox>
               </right>
            </BorderPane>
         </children>
      </VBox>
   </top>
   <center>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <center>
            <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Pane fx:id="mapPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="481.0" prefWidth="617.0" style="-fx-background-color: #ECECEC;" />
                  <DialogPane fx:id="intersectionInfoDialog" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="112.0" prefWidth="339.0" style="-fx-background-color: ffffff;" visible="false">
                     <header>
                        <HBox alignment="TOP_RIGHT" prefHeight="19.0" prefWidth="421.0">
                           <children>
                              <Button mnemonicParsing="false" onMouseClicked="#hideIntersectionInfoDialog" prefHeight="28.0" prefWidth="29.0" text="x" />
                           </children>
                           <padding>
                              <Insets right="2.0" top="2.0" />
                           </padding>
                        </HBox>
                     </header>
                     <content>
                        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="0.0" prefWidth="339.0">
                           <children>
                              <Button fx:id="prevDeliveryPointInfo" layoutX="3.0" layoutY="24.0" mnemonicParsing="false" onMouseClicked="#prevDeliveryPointInfoDialog" prefHeight="24.0" prefWidth="0.0" text="&lt;" />
                              <Text fx:id="deliveryWindow" layoutX="55.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Delivery Window : XXh-XXh" textAlignment="CENTER" wrappingWidth="228.28125" />
                              <Button fx:id="nextDeliveryPointInfo" layoutX="308.0" layoutY="22.0" mnemonicParsing="false" onMouseClicked="#nextDeliveryPointInfoDialog" prefHeight="24.0" prefWidth="29.0" text="&gt;" />
                              <Button layoutX="308.0" layoutY="67.0" mnemonicParsing="false" onMouseClicked="#addDeliveryPointDialog" prefHeight="24.0" prefWidth="29.0" text="+" />
                           </children>
                        </AnchorPane>
                     </content>
                  </DialogPane>
                  <DialogPane fx:id="newDeliveryRequestDialogPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="135.0" prefWidth="339.0" style="-fx-background-color: ffffff;" visible="false">
                     <header>
                        <HBox alignment="TOP_RIGHT" prefHeight="30.0" prefWidth="339.0">
                           <children>
                              <Button fx:id="closeAddDeliveryRequestDialogPane" mnemonicParsing="false" onAction="#closeAddDeliveryRequestDialogPane" prefHeight="28.0" prefWidth="29.0" text="x" />
                           </children>
                           <padding>
                              <Insets right="2.0" top="2.0" />
                           </padding>
                        </HBox>
                     </header>
                     <content>
                        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="0.0" prefWidth="339.0">
                           <children>
                              <VBox prefHeight="59.0" prefWidth="339.0">
                                 <children>
                                    <HBox prefHeight="25.0" prefWidth="339.0">
                                       <children>
                                          <Text fx:id="deliveryWindow1" strokeType="OUTSIDE" strokeWidth="0.0" text="Courier:" textAlignment="CENTER" wrappingWidth="228.28125" />
                                          <ChoiceBox fx:id="courierChoiceBox" prefWidth="150.0" />
                                       </children>
                                    </HBox>
                                    <HBox prefHeight="27.0" prefWidth="338.0">
                                       <children>
                                          <Text fx:id="deliveryWindow11" strokeType="OUTSIDE" strokeWidth="0.0" text="Time window:" textAlignment="CENTER" wrappingWidth="228.28125" />
                                          <ChoiceBox fx:id="timeWindowChoiceBox" prefWidth="150.0" />
                                       </children>
                                    </HBox>
                                    <HBox alignment="TOP_CENTER" prefHeight="20.0" prefWidth="339.0">
                                       <children>
                                          <Button mnemonicParsing="false" onMouseClicked="#addDeliveryRequest" prefHeight="24.0" prefWidth="29.0" text="Add" />
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                     </content>
                  </DialogPane>
               </children>
            </StackPane>
         </center>
         <left>
            <VBox prefHeight="300.0" prefWidth="300.0" BorderPane.alignment="CENTER">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Delivery details" textAlignment="CENTER" wrappingWidth="300.0">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                  </Text>
                  <Text fx:id="deliveryDetailsText" layoutX="10.0" layoutY="22.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
                     <VBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </VBox.margin>
                  </Text>
                  <Text layoutX="10.0" layoutY="22.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Itinerary" textAlignment="CENTER" wrappingWidth="300.0">
                     <VBox.margin>
                        <Insets bottom="10.0" top="15.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Text>
                  <Text fx:id="itineraryText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
                     <VBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </VBox.margin>
                  </Text>
               </children>
            </VBox>
         </left>
      </BorderPane>
   </center>
   <left>
      <VBox prefHeight="524.0" prefWidth="186.0" BorderPane.alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Delivery details" textAlignment="CENTER" wrappingWidth="200.0">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Text>
            <Text layoutX="10.0" layoutY="22.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </Text>
            <Text layoutX="10.0" layoutY="22.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Itinerary" textAlignment="CENTER" wrappingWidth="200.0">
               <VBox.margin>
                  <Insets bottom="10.0" top="15.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </Text>
         </children>
      </VBox>
   </left>
</BorderPane>
